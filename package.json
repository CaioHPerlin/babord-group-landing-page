{
  "name": "the-keyboard-store",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "start": "next start",
    "start:dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "prepare": "husky"
  },
  "dependencies": {
    "motion": "^12.23.22",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-plugin-react-hooks": "^6.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "typescript": "^5"
  },
  "lint-staged": {
    "**/*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
